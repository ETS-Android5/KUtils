<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <!-- https://blog.csdn.net/zhe_d/article/details/83934231 -->
    <!-- 
    remote: 远程代码库配置
    name: 远程仓库名，在使用 git clone 时默认为 origin
    fetch: 远程仓库地址，用户和 project 节点中的 name 一起确定项目地址
    -->
    <remote name="xUtil"
            fetch="ssh://git@git.local.sxbcar.com:10022/android/xUtils3.git"
            revision="master" />

    <!--
    project: 单个代码库配置
    name: 项目地址，和指定的 remote 节点中的 fetch 一起组成仓库地址（fetch + name）
    group: 项目所属分组，可选
    path: 项目拉取到本地后所在目录
    remote: 指定所在远程仓库
    revision: 指定代码分支
    -->
    <project path="xx" name="mytest" groups="tt" remote="xUtil"/>
</manifest>